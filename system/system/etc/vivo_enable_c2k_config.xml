  <item model="PD1934BF_EX" hw_id="0101" match_rule="0,4"/>
  <item model="PD1987F_EX" hw_id="000,001,010,100" match_rule="1,3"/>
  <item model="PD1987BF_EX" hw_id="000,001,010,100" match_rule="1,3"/>
  <item model="PD2014F_EX" hw_id="**110***" match_rule="-1,-1"/>
  <item model="PD2036F_EX" hw_id="**001*,**100*" match_rule="-1,-1"/>
  <item model="PD2069F_EX" hw_id="*************" match_rule="-1,-1"/>
  <item model="PD2074F_EX" hw_id="**001*,**100*" match_rule="-1,-1"/>
</config>