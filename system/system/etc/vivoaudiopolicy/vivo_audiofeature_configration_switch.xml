<!-- value off:false,on:true
The secondary switch for example persist.sys.audio.vapc.playback.bkplaybackenable
Only after the switch of the previous level is opened, the switch of the next level can be opened, otherwise it is closed by default
-->
<switches>
	<switch name="persist.vivo.audio.feature.directional_playback.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.micinfo_vcode.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.speaker_vcode.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.spkbvm_vcode.enable" value="on"/>
	<switch name="persist.vivo.audio.feature.callmute_vcode.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.muteplay_disablepa.enable" value="on"/>
	<switch name="persist.vivo.audio.feature.vivomediaresource_monitor.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.bt_recorder.enable" value="off"/>
	<switch name="persist.vivo.audio.feature.restorevolume.enable" value="off"/>
</switches>
