        launcher:className="com.google.android.apps.subscriptions.red.LauncherActivity"
        launcher:packageName="com.google.android.apps.subscriptions.red"
        launcher:screen="0"
        launcher:x="10"
        launcher:y="0"/>

    </folder>

    <!-- 第2屏 -->
    <!-- Google Files -->
