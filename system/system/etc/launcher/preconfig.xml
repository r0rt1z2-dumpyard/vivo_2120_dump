        launcher:container="-100"
        launcher:packageName="com.vivo.website"
        launcher:screen="1"
        launcher:x="3"
        launcher:y="5" />

    <!-- vivo官网 -->
    <favorite
        launcher:className="com.vivo.website.activity.SplashActivity"
        launcher:container="-100"
        launcher:packageName="com.vivo.website.gdpr"
        launcher:screen="1"
        launcher:x="3"
        launcher:y="5" />

    <!-- Facebook -->
    <favorite
        launcher:className="com.facebook.lite.MainActivity"
        launcher:container="-100"
        launcher:packageName="com.facebook.lite"
        launcher:screen="1"
