<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.bbk.launcher2"
    launcher:screenCount="2">
	<iconutils
        launcher:iconUtilsScaleFactorMax="1.16"
        launcher:iconUtilsScaleFactorMin="0.85"
        launcher:iconUtilsBgWScaleFactor="1.01"
        launcher:iconUtilsBgHScaleFactor="1.01"/>
		
    <!-- Global Search -->
    <globalsearch
        launcher:globalSearchBgBlurStyle="true"
        launcher:globalSearchEnable="true" />

    <!-- default workspace -->
    <celllayout
        launcher:cellLayoutCellX="5"
        launcher:cellLayoutCellY="6"
		launcher:drawerCellLayoutCellX="5"
        launcher:drawerCellLayoutCellY="6" />
		
    <!-- support hiboard or not -->
    <hiboard launcher:supportHiboard="true"/>

    <!-- default theme -->
    <theme launcher:theme="android:raw/themes6" />

    <!-- default scroll effect -->
    <scroll launcher:scrollEffect="0" />
	
	<!-- use new enter app animation or not -->
    <animation launcher:useNewAnim="false" />
	
    <!-- show hide icon or not -->
    <hideicon launcher:showHideIcon="false" />
	
    <!-- hide shortcut title or not -->
    <hideshortcuttitle launcher:hideShortCutTitle="true" />

    <!-- icon data check enable or not -->
    <icondatacheck launcher:iconDataCheckEnable="true" />

	<!-- icon style support or not -->
    <iconstylesupport launcher:iconstylesupport="true" />
	
	<!-- smart replace icon enable or not -->
    <smartreplaceicon launcher:smartReplaceIconEnable="true" />
	
    <!-- icon style support or not -->
    <iconstylesupport launcher:iconstylesupport="true" />
	
    <!-- layout switch support or not -->
    <layoutswitchsupport launcher:layoutSwitchSupport="true" />
	
    <!-- 布局切换需要显示的布局类型 -->
	<layoutswitch launcher:layout="4X6" />
	<layoutswitch launcher:layout="5X6" />

    <!-- scene menu -->
    <scenemenu launcher:sceneMenu="true" />

    <!-- default scene -->
    <scene launcher:componentName="com.bbk.scene.tech/com.bbk.scene.tech.MainActivity" />
	
    <!-- 是否支持布局页面RTL -->
    <localedirection launcher:localedirection="true" />

    <!-- 是否逐层飞出动效 -->
    <unlockFlyoutOnebyone launcher:unlockFlyoutOnebyone="true"/>

    <!-- 新图标动画开关，register方式 -->
    <registeranimation launcher:registeranimation="false" />

    <!-- 设窗圆角开关 -->
    <roundradius launcher:roundradius="false" />

	<!-- default launcher style -->
    <launcherStyle launcher:launcherStyle="1" />
	
	<!-- 动态模糊开关 -->
