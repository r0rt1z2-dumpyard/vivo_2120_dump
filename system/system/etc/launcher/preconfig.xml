<favorites xmlns:launcher="http://schemas.android.com/apk/res-auto/com.android.launcher3">

    <!-- google go 搜索 -->
    <appwidget
        launcher:className="com.google.android.apps.searchlite.widget.SearchLiteWidgetProvider"
        launcher:container="-100"
        launcher:packageName="com.google.android.apps.searchlite"
        launcher:screen="0"
        launcher:spanX="5"
        launcher:spanY="1"
        launcher:x="0"
        launcher:y="0"/>

        <!-- 时钟组件 -->		
    <appwidget
        launcher:className="com.vivo.timerwidget.clockwidget.ClockWidgetProvider"
        launcher:container="-100"
        launcher:packageName="com.vivo.timerwidget"
        launcher:screen="0"
        launcher:spanX="5"
        launcher:spanY="1"
        launcher:x="0"
        launcher:y="1"/>

    <!-- 常驻栏 -->
    <!-- 电话 -->
    <resolve
        launcher:container="-101"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0" >
        <favorite launcher:uri="#Intent;action=android.intent.action.DIAL;end" />
        <favorite launcher:uri="tel:123" />
        <favorite launcher:uri="#Intent;action=android.intent.action.CALL_BUTTON;end" />
    </resolve>

    <!-- 电话 -->
    <favorite
        launcher:className="com.android.dialer.TwelveKeyDialer"
        launcher:container="-101"
        launcher:packageName="com.android.dialer"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0"/>

    <!-- 信息 -->
    <resolve
        launcher:container="-101"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0" >
        <favorite launcher:uri="#Intent;action=android.intent.action.MAIN;category=android.intent.category.APP_MESSAGING;end" />
        <favorite launcher:uri="sms:" />
        <favorite launcher:uri="smsto:" />
        <favorite launcher:uri="mms:" />
        <favorite launcher:uri="mmsto:" />
    </resolve>

    <!-- 信息 -->
    <favorite
        launcher:className="com.android.mms.ui.ConversationList"
        launcher:container="-101"
        launcher:packageName="com.android.mms"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="0"/>

    <!-- 浏览器 -->
    <resolve
        launcher:container="-101"
        launcher:screen="2"
        launcher:x="2"
        launcher:y="0" >
        <favorite
            launcher:uri="#Intent;action=android.intent.action.MAIN;category=android.intent.category.APP_BROWSER;end" />
        <favorite launcher:uri="http://www.example.com/" />
    </resolve>

    <!-- Chrome -->
    <favorite
        launcher:className="com.google.android.apps.chrome.Main"
        launcher:container="-101"
        launcher:packageName="com.android.chrome"
        launcher:screen="2"
        launcher:x="2"
        launcher:y="0"/>

    <!-- 谷歌商店 -->
    <favorite
        launcher:className="com.android.vending.AssetBrowserActivity"
        launcher:container="-101"
        launcher:packageName="com.android.vending"
        launcher:screen="3"
        launcher:x="3"
        launcher:y="0"/>

    <!-- 相机 -->
    <resolve
        launcher:container="-101"
        launcher:screen="4"
        launcher:x="4"
        launcher:y="0" >
        <favorite launcher:uri="#Intent;action=android.media.action.STILL_IMAGE_CAMERA;end" />
        <favorite launcher:uri="#Intent;action=android.intent.action.CAMERA_BUTTON;end" />
    </resolve>

    <!-- 第1屏 -->
