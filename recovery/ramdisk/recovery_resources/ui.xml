					<row1 key01="1" key02="2" key03="3" key04="4" key05="5" key06="6" key07="7" key08="8" key09="9" key10="0"/>
					<row2 key01="Q" key02="W" key03="E" key04="R" key05="T" key06="Y" key07="U" key08="I" key09="O" key10="P"/>
					<row3 key01="52:" key02="A" key03="S" key04="D" key05="F" key06="G" key07="H" key08="J" key09="K" key10="L" key11="52:"/>
					<row4 key01="155:layout1" key02="Z" key03="X" key04="C"  key05="V"  key06="B"  key07="N"  key08="M" key09="155:c:8"/>
					<row5 key01="246:layout3"  key02="540: " key03="246:action"/>
					<keylabel key="0:layout1" text="Shift" resource="shift_fill"/>
