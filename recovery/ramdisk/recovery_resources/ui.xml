		<variable name="vivo_downloading_tips_x2" value="700"/>      <!--  -->
		<variable name="vivo_install_battery_tips_y" value="1260"/>
		<variable name="vivo_downloading_progress_bar_y" value="1251"/>          <!-- 139dp  -->
		<variable name="vivo_downloading_progress_tips_y"   value="1309"/>
		<variable name="vivo_downloading_progress_anima_y" value="1368"/>        <!-- 152dp  -->
		<variable name="vivo_downloading_btn1_y" value="1557"/>        <!-- 173dp  -->
		<variable name="vivo_downloading_btn2_y" value="1784"/>        <!-- 198.33dp  -->
	</variables>

	<templates>
		<template name="page">
			<background color="%background_color%"/>

			<image>
				<condition var1="g_current_page" op="!=" var2="main"/>
				<condition var1="g_current_page" op="!=" var2="recovery"/>
				<condition var1="g_current_page" op="!=" var2="survival"/>
				<condition var1="g_current_page" op="!=" var2="fastboot"/>
				<condition var1="tt_busy" var2="1"/>
				<placement x="%back_btn_x%" y="%header_y%"/>
				<image resource="back"/>
			</image>

			<button>
				<condition var1="g_current_page" op="!=" var2="main"/>
				<condition var1="g_current_page" op="!=" var2="recovery"/>
				<condition var1="g_current_page" op="!=" var2="survival"/>
				<condition var1="g_current_page" op="!=" var2="fastboot"/>
				<condition var1="tt_busy" var2="0"/>
				<placement x="%back_btn_x%" y="%header_y%"/>
				<image resource="back"/>
				<action function="key">back</action>
			</button>

			<batteryview>
				<condition var1="property.recovery.state.screen_shape" op="!=" var2="two_holes_right"/>
				<placement x="%battery_pos_x%" y="%row1_header_y%"/>
				<resource name="vivo_battery"/>
				<speed fps="3" render="2"/>
				<loop frame="1"/>
			</batteryview>

			<batteryview>
				<condition var1="property.recovery.state.screen_shape" op="=" var2="two_holes_right"/>
				<placement x="%battery_pos_wakong_screen_x%" y="%row1_header_y%"/>
				<resource name="vivo_battery"/>
				<speed fps="3" render="2"/>
				<loop frame="1"/>
			</batteryview>
		</template>

		<template name="confirm_box_big_temp">
			<image>
				<image resource="confirm_box_big_bg"/>
				<placement x="%center_x%" y="%confirmbox_big_y%" placement="4"/>
			</image>

			<text>
				<placement x="%center_x%" y="%confirmbox_big_txt_hdr_y%"  placement="5"/>
				<font resource="font_h" color="%black_color%"/>
				<text>%tt_text1%</text>
			</text>

			<!-- <text>
				<placement x="%center_x%" y="%confirmbox_big_txt_tips_y%"  placement="5"/>
				<font resource="font_l" color="%black_color%"/>
				<text>{@wlan_repair_tips=...}</text>
			</text> -->

			<textbox>
				<placement x="%center_x%" y="%confirmbox_big_txt_tips_y%" w="%confirmbox_big_w%" h="%confirmbox_big_h%" placement="4"/>
				<background color="%transparent%"/>
				<font resource="font_l" spacing="%fileselector_spacing%" color="%black_color%"/>
				<view_style x="%center_x%" placement="5"/>
				<text>%tt_text2%</text>
			</textbox>
		</template>

		<template name="progress_bar">
			<xprogressbar>
				<condition var1="tt_use_xprogressbar" op="!=" var2="0"/>
				<placement x="%center_x%" y="%progress_bar_y%" placement="4"/>
				<resource empty="progress_empty" full="progress_full"/>
				<data name="ui_xprogress"/>
			</xprogressbar>

			<progressbar>
				<condition var1="tt_use_xprogressbar" var2="0"/>
				<placement x="%center_x%" y="%progress_bar_y%" placement="4"/>
				<resource empty="progress_empty" full="progress_full"/>
				<data name="ui_progress"/>
			</progressbar>

			<animation>
				<placement x="%center_x%" y="%progress_anima_y%" placement="4"/>
				<resource name="progress"/>
				<speed fps="30" render="2"/>
				<loop frame="1"/>
			</animation>
		</template>

		<template name="console">
			<console>
				<placement x="%indent%" y="%row2_y%" w="%content_width%" h="%console_height%"/>
			</console>

			<fill color="%fileselector_linecolor%">
				<placement x="%indent%" y="row2_y" w="%content_width%" h="input_line_width" placement="1"/>
			</fill>

			<fill color="%fileselector_linecolor%">
				<placement x="%indent%" y="row17_y" w="%content_width%" h="input_line_width"/>
			</fill>
		</template>

		<template name="vivodigitkeyboardtemplate">
			<keyboard>
				<condition var1="tt_hide_kb" var2="0"/>
				<placement x="%center_x%" y="%digit_keyboard_y%" w="%digit_keyboard_w%" h="%digit_keyboard_h%" placement="4"/>
				<keymargin x="8" y="8"/>
				<background color="%background_color%"/>
				<key-alphanumeric color="%background_color%" font="keylabel" textcolor="#EEEEEE"/>
				<key-other color="%background_color%" font="keylabel-small" textcolor="#5b5b5bff"/>
				<extra-prop rowseparationline="1" rowseparationcolor="%separator_line_color%" rowseparationheight="%separator_line_h%"/>
				<!-- <longpress font="keylabel-longpress" textcolor="#5b5b5bff" x="12" y="0"/> -->
				<!-- <keylabel key="0:c:8" text="Bksp" resource="backspace"/>
				<keylabel key="0:action" text="Enter" resource="enter"/>
				<keylabel key=" " text="Space" resource="space"/>  -->
				<highlight color="#111111"/>
				<capshighlight color="%highlight_color%"/>

				<layout1>
					<keysize height="186" width="243"/>
					<row1 key01="1" key02="2" key03="3" />
					<row2 key01="4" key02="5" key03="6" />
					<row3 key01="7" key02="8" key03="9" />
					<row4 key01="243:" key02="0" key03="243:" />
				</layout1>
			</keyboard>
		</template>

		<template name="vivodcompexkeyboardtemplate">
			<keyboard>
				<condition var1="tt_hide_kb" var2="0"/>
				<placement x="%center_x%" y="%compex_keyboard_y%" w="%compex_keyboard_w%" h="%compex_keyboard_h%" placement="4"/>
				<keymargin x="8" y="12"/>
				<background color="%background_color%"/>
				<key-alphanumeric color="%background_color%" font="keylabel" textcolor="#EEEEEE"/>
				<key-other color="%background_color%" font="keylabel-small" textcolor="#EEEEEE"/>
				<!-- <longpress font="keylabel-longpress" textcolor="#5b5b5bff" x="12" y="0"/> -->
				<!-- <keylabel key="0:c:8" text="Bksp" resource="backspace"/>
				<keylabel key="0:action" text="Enter" resource="enter"/>   -->
				<keylabel key="0:c:8" text="Bksp" resource="backspace"/>
				<highlight color="#4A4A4A"/>
				<capshighlight color="%highlight_color%"/>
				<extra-prop rowseparationline="0" rowseparationcolor="%separator_line_color%" rowseparationheight="%separator_line_h%" keyinbox='1'/>

				<layout1>
					<keysize height="124" width="103"/>
					<row1 key01="1" key02="2" key03="3" key04="4" key05="5" key06="6" key07="7" key08="8" key09="9" key10="0"/>
