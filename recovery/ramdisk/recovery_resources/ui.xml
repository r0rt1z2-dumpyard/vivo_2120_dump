					<row1 key01="1" key02="2" key03="3" key04="4" key05="5" key06="6" key07="7" key08="8" key09="9" key10="0"/>
					<row2 key01="Q" key02="W" key03="E" key04="R" key05="T" key06="Y" key07="U" key08="I" key09="O" key10="P"/>
					<row3 key01="47:" key02="A" key03="S" key04="D" key05="F" key06="G" key07="H" key08="J" key09="K" key10="L" key11="47:"/>
					<row4 key01="141:layout1" key02="Z" key03="X" key04="C"  key05="V"  key06="B"  key07="N"  key08="M" key09="141:c:8"/>
					<keylabel key="0:layout1" text="Shift" resource="shift_fill"/>
				</layout2>
			</keyboard>
		</template>

		<template name="vivokeyboardtemplate">
			<keyboard>
				<condition var1="tt_hide_kb" var2="0"/>
				<placement x="%center_x%" y="%compex_keyboard_y%" w="1032" h="805" placement="4"/>
				<keymargin x="8" y="12"/>
				<background color="%background_color%"/>
				<key-alphanumeric color="%background_color%" font="keylabel" textcolor="#EEEEEE"/>
				<key-other color="%background_color%" font="keylabel-small" textcolor="#EEEEEE"/>
				<longpress font="keylabel-longpress" textcolor="#5b5b5bff" x="12" y="0"/>
				<keylabel key="0:c:8" text="Bksp" resource="backspace"/>
				<keylabel key="0:action" text="{@compelte_btn}"/>
				<keylabel key=" " text="Space" resource="space"/>
				<highlight color="#4A4A4A"/>
				<capshighlight color="%highlight_color%"/>
