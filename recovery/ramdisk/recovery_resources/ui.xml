					<row4 key01="155:layout2" key02="z" key03="x" key04="c"  key05="v"  key06="b"  key07="n"  key08="m" key09="155:c:8"/>
					<row5 key01="246:layout3"  key02="540: " key03="246:action"/>
					<keylabel key="0:layout2" text="Shift" resource="shift"/>
					<keylabel key="0:layout3" text="{@symol_btn}"/>
				</layout1>
				<layout2>
